<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="model.vo.CloudVO" table="Cloud">
		<id name="fileId" column="fileId">
			<generator class="identity" />
		</id>
		<property name="memberId" column="memberId" />
		<property name="fileName" column="fileName" />
		<property name="fileType" column="fileType" />
		<property name="filePath" column="filePath" />
		<property name="fileSize" column="fileSize" />
		<property name="modifyTime" column="modifyTime" />
	</class>
</hibernate-mapping>